    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/testing": "^9.4.3",
    "@openrouter/sdk": "^0.1.3",
    "@prisma/client": "^6.19.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.13.2",
    "body-parser": "^2.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.6.1",
    "hbs": "^4.2.0",
    "joi": "^18.0.2",
    "marked": "^17.0.0",
    "prisma": "^6.19.0",
